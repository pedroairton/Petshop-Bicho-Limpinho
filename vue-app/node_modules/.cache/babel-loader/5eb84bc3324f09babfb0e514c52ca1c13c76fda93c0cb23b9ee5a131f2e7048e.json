   <select name=\"pet\" id=\"petSelect\">\r\n                        <option value=\"0\" selected disabled>Selecione o pet</option>\r\n                        <option value=\"1\">Pet1</option>\r\n                        <option value=\"2\">Pet2</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"input\">\r\n                    <label for=\"servSelect\">Selecione o serviço</label>\r\n                    <select name=\"serv\" id=\"servSelect\">\r\n                        <option value=\"0\" selected disabled>Selecione o serviço</option>\r\n                        <option value=\"1\">Banho</option>\r\n                        <option value=\"2\">Tosa</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"input\">\r\n                    <label for=\"calend\">Selecione um horário disponível</label>\r\n                    <div id=\"calend\">\r\n                        <input id=\"date\" type=\"date\" />\r\n                        <FullCalendar \r\n                        :events=\"events\"\r\n                        :headerToolbar=\"headerToolbar\"\r\n                        :options=\"calendarOptions\"\r\n                        @eventClick=\"handleEventClick\" />\r\n                    </div>\r\n                </div>\r\n                <hr>\r\n                <div class=\"input\">\r\n                    <label for=\"infos\">Informações adicionais (Opcional)</label>\r\n                    <textarea name=\"infos\" id=\"infos\" rows=\"10\" cols=\"30\"></textarea>\r\n                </div>\r\n                <div class=\"input\">\r\n                    <label for=\"telefone\">Telefone</label>\r\n                    <input type=\"text\" id=\"telefone\">\r\n                </div>\r\n                <button class=\"btn-enviar\" type=\"submit\">Agendar</button>\r\n            </form>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\n    import FullCalendar from '@fullcalendar/vue3'\r\n    import dayGridPlugin from '@fullcalendar/daygrid'\r\n    import interactionPlugin from '@fullcalendar/interaction'\r\n\r\n    export default {\r\n        name: 'AgendamentoPage',\r\n        components: {\r\n            FullCalendar\r\n        },\r\n        data(){\r\n            return {\r\n                headerToolbar: {\r\n                    left: 'prev,next today',\r\n                    center: 'title',\r\n                    right: 'dayGridMonth, dayGridWeek, dayGridDay'\r\n                },\r\n                calendarOptions: {\r\n                    initialView: 'dayGridMonth',\r\n                    plugins: [dayGridPlugin, interactionPlugin ],\r\n                    initialView: 'dayGridMonth',\r\n                    events: [\r\n                        {title: 'Evento 1', date: '2024-10-23'},\r\n                        {title: 'Evento 2', date: '2024-10-24'},\r\n                    ],\r\n                }\r\n            }\r\n        },\r\n        methods: {\r\n            enviaForm(){\r\n                console.log('enviado')\r\n            },\r\n            handleEventClick(info){\r\n                alert('Evento: ' + info.event.title)\r\n            }\r\n        },\r\n    }\r\n</script>"],"mappings":"AAoDI,OAAOA,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,aAAY,MAAO,uBAAsB;AAChD,OAAOC,iBAAgB,MAAO,2BAA0B;AAExD,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACRJ;EACJ,CAAC;EACDK,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,aAAa,EAAE;QACXC,IAAI,EAAE,iBAAiB;QACvBC,MAAM,EAAE,OAAO;QACfC,KAAK,EAAE;MACX,CAAC;MACDC,eAAe,EAAE;QACbC,WAAW,EAAE,cAAc;QAC3BC,OAAO,EAAE,CAACX,aAAa,EAAEC,iBAAgB,CAAG;QAC5CS,WAAW,EAAE,cAAc;QAC3BE,MAAM,EAAE,CACJ;UAACC,KAAK,EAAE,UAAU;UAAEC,IAAI,EAAE;QAAY,CAAC,EACvC;UAACD,KAAK,EAAE,UAAU;UAAEC,IAAI,EAAE;QAAY,CAAC;MAE/C;IACJ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,SAASA,CAAA,EAAE;MACPC,OAAO,CAACC,GAAG,CAAC,SAAS;IACzB,CAAC;IACDC,gBAAgBA,CAACC,IAAI,EAAC;MAClBC,KAAK,CAAC,UAAS,GAAID,IAAI,CAACE,KAAK,CAACT,KAAK;IACvC;EACJ;AACJ","ignoreList":[]},"metadata":{},"sour