{"ast":null,"code":"document.addEventListener(\"DOMContentLoaded\", function () {\n  const swiperEl = document.querySelector('swiper-container');\n  if (swiperEl) {\n    const params = {\n      injectStyles: [`\n        .swiper-pagination-bullet {\n          width: 20px;\n          height: 20px;\n          text-align: center;\n          line-height: 20px;\n          font-size: 12px;\n          color: #000;\n          opacity: 1;\n          background: rgba(0, 0, 0, 0.2);\n        }\n\n        .swiper-pagination-bullet-active {\n          color: #fff;\n          background: #007aff;\n        }\n      `],\n      pagination: {\n        clickable: true,\n        renderBullet: function (index, className) {\n          return '<span class=\"' + className + '\">' + (index + 1) + \"</span>\";\n        }\n      }\n    };\n    Object.assign(swiperEl, params);\n    swiperEl.initialize();\n  } else {\n    console.error(\"swiper-container element not found.\");\n  }\n});\nimport NavBar from './NavBar.vue';\nexport default {\n  name: 'HomePage',\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      inputNome: '',\n      contador: 0\n    };\n  },\n  methods: {\n    submitForm(event) {\n      event.preventDefault();\n      console.log(this.inputNome);\n    },\n    increment(event) {\n      event.preventDefault();\n      this.contador++;\n    }\n  }\n};","map":{"version":3,"names":["document","addEventListener","swiperEl","querySelector","params","injectStyles","pagination","clickable","renderBullet","index","className","Object","assign","initialize","console","error","NavBar","name","components","data","inputNome","contador","methods","submitForm","event","preventDefault","log","increment"],"sources":["C:\\Users\\pedro\\Documents\\GitHub\\vue\\vue-app\\src\\components\\HomePage.vue"],"sourcesContent":["<style>\r\n.swiper-pagination-bullet{\r\n    width: 35px;\r\n    height: 35px;\r\n}\r\n</style>\r\n<template>\r\n    <NavBar />\r\n    <section>\r\n        <!-- SWIPER C