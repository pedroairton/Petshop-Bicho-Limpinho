n\r\n\r\n    export default {\r\n        name: 'HomePage',\r\n        components: {\r\n            NavBar,\r\n        },\r\n        data(){\r\n            return{\r\n            inputNome: '',\r\n            contador: 0,\r\n            }\r\n        },\r\n        methods: {\r\n            submitForm(event){\r\n            event.preventDefault();\r\n            console.log(this.inputNome)\r\n            },\r\n            increment(event){\r\n            event.preventDefault()\r\n            this.contador++\r\n            }\r\n        }\r\n    }\r\n\r\n</script>"],"mappings":"AAkCIA,QAAQ,CAACC,gBAAgB,CAAC,kBAAkB,EAAE,YAAW;EAC3D,MAAMC,QAAO,GAAIF,QAAQ,CAACG,aAAa,CAAC,kBAAkB,CAAC;EAE3D,IAAID,QAAQ,EAAE;IACZ,MAAME,MAAK,GAAI;MACbC,YAAY,EAAE,CAAC;;;;;;;;;;;;;;;;OAgBd,CAAC;MACFC,UAAU,EAAE;QACVC,SAAS,EAAE,IAAI;QACfC,YAAY,EAAE,SAAAA,CAAUC,KAAK,EAAEC,SAAS,EAAE;UACxC,OAAO,eAAc,GAAIA,SAAQ,GAAI,IAAG,IAAKD,KAAI,GAAI,CAAC,IAAI,SAAS;QACrE;MACF;IACF,CAAC;IAEDE,MAAM,CAACC,MAAM,CAACV,QAAQ,EAAEE,MAAM,CAAC;IAC/BF,QAAQ,CAACW,UAAU,CAAC,CAAC;EACvB,OAAO;IACLC,OAAO,CAACC,KAAK,CAAC,qCAAqC,CAAC;EACtD;AACF,CAAC,CAAC;AAGE,OAAOC,MAAK,MAAO,cAAa;AAGhC,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACNC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE;IACV;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAACC,KAAK,EAAC;MACjBA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtBX,OAAO,CAACY,GAAG,CAAC,IAAI,CAACN,SAAS;IAC1B,CAAC;IACDO,SAASA,CAACH,KAAK,EAAC;MAChBA,KAAK,CAACC,cAAc,CAAC;MACrB,IAAI,CAACJ,QAAQ,EAAC;IACd;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}